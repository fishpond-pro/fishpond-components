datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider        = "prisma-client-js"
  output          = "./customPrismaClient/client"
  previewFeatures = ["fullTextSearch", "fullTextIndex"]
}

model RSSHub_Source_Item {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
